# ——————————————————————————————————————————————————————————————————————————
# Obtention de la liste des images
# ——————————————————————————————————————————————————————————————————————————
liste.images <- Full.Source(paste(path,"/images", sep=""),ignored.files.vector=NULL,print.inside.message = FALSE, type="pdf")
n.images <- length(liste.images)
name.broken <- name_break(liste.images,n.images)
liste.noms <- name.broken$liste.noms
liste.img.finale <- name.broken$liste.img.finale
# Selection des dates
dates <- c(as.Date(today()) + 0:40)
n.dates <- length(dates)
Full.chart <- gen.sport(strsplit(as.character(liste.img.finale$V2[1]), ".", fixed = TRUE)[[1]][1],liste.noms[1],dates=dates,nbrows=nbrows)
for (i in 2:n.images){
if(i==n.images){total.line=TRUE}else{total.line <- FALSE}
Full.chart <- paste(
Full.chart,
gen.sport(
image = strsplit(as.character(liste.img.finale$V2[i]), ".", fixed = TRUE)[[1]][1],
titre = liste.noms[i],
len = n.dates,
nbrows=nbrows,
total.line=total.line
),
sep="",
collapse=""
)
}
